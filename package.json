{
  "name": "wallet-core",
  "version": "1.0.0",
  "description": "High performance transactional ledger with strict consistency",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "test": "jest --detectOpenHandles --verbose",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\""
  },
  "keywords": [
    "ledger",
    "acid",
    "fastify",
    "kysely"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^5.22.0",
    "dotenv": "^17.2.3",
    "fastify": "^4.29.1",
    "fastify-type-provider-zod": "^1.1.9",
    "kysely": "^0.27.6",
    "pg": "^8.16.3",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.27",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.7.4",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^1.2.1"
  },
  "type": "commonjs"
}